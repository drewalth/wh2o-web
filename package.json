{
  "name": "wh2o-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "antd": "4.19.3",
    "classnames": "2.3.1",
    "lodash.debounce": "4.0.8",
    "luxon": "2.3.1",
    "mapbox-gl": "2.8.1",
    "react": "^17.0.2",
    "react-apexcharts": "1.4.0",
    "react-dom": "^17.0.2",
    "react-router-dom": "6.2.2",
    "react-scripts": "5.0.0",
    "web-vitals": "2.1.4"
  },
  "scripts": {
    "start": "export PORT=8080 && react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint",
    "pretty": "prettier ./src/ --config prettier.config.js -w"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "no-unused-vars": "error"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.3",
    "@testing-library/react": "12.1.4",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.4.1",
    "@types/luxon": "2.3.1",
    "@types/node": "16.11.26",
    "@types/react": "17.0.42",
    "@types/react-dom": "18.0.1",
    "@types/react-router-dom": "5.3.3",
    "eslint": "7.32.0",
    "eslint-config-react-app": "7.0.1",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "sass": "1.50.0",
    "typescript": "4.6.3"
  }
}
